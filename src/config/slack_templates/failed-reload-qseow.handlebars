{
    "blocks": [{
            "type": "header",
            "text": {
                "type": "plain_text",
                "text": "Qlik Sense reload failed"
            }            
        },
        {
            "type": "divider"
        },
        {
            "type": "section",
            "fields": [{
                    "type": "mrkdwn",
                    "text": "*Task name:*\n{{taskName}}"
                },
                {
                    "type": "mrkdwn",
                    "text": "*Task ID:*\n{{{taskId}}}"
                },
                {
                    "type": "mrkdwn",
                    "text": "*App name:*\n{{appName}}"
                },
                {
                    "type": "mrkdwn",
                    "text": "*App ID:*\n{{{appId}}}"
                },
                {
                    "type": "mrkdwn",
                    "text": "*App owner:*\n{{{appOwnerName}}}"
                },
                {
                    "type": "mrkdwn",
                    "text": "*App owner email:*\n{{{appOwnerEmail}}}"
                },
                {
                    "type": "mrkdwn",
                    "text": "*App owner user:*\n{{appOwnerUserDirectory}}/{{appOwnerUserId}}"
                },
                {
                    "type": "mrkdwn",
                    "text": "*Link to app:*\n{{appUrl}}"
                }
            ]
        },
        {
            "type": "section",
            "fields": [{
                    "type": "mrkdwn",
                    "text": "*Reload on node:*\n{{executingNodeName}}"
                },
                {
                    "type": "mrkdwn",
                    "text": "*User:*\n{{user}}"
                },
                {
                    "type": "mrkdwn",
                    "text": "*Execution result:*\n{{executionStatusText}}"
                },
                {
                    "type": "mrkdwn",
                    "text": "*Execution result code:*\n{{executionStatusNum}}"
                }
            ]
        },
        {
            "type": "section",
            "fields": [{
                    "type": "mrkdwn",
                    "text": "*Duration:*\n{{executionDuration.hours}} hours, {{executionDuration.minutes}} minutes, {{executionDuration.seconds}} seconds"
                },
                {
                    "type": "mrkdwn",
                    "text": " "
                },
                {
                    "type": "mrkdwn",
                    "text": "*Execution started:*\n{{executionStartTime.startTimeLocal1}}"
                },
                {
                    "type": "mrkdwn",
                    "text": "*Execution ended:*\n{{executionStopTime.stopTimeLocal1}}"
                }
            ]
        },
        {
            "type": "actions",
            "elements": [
                {
                    "type": "button",
                    "text": {
                        "type": "plain_text",
                        "emoji": true,
                        "text": "Open QMC"
                    },
                    "style": "primary",
                    "url": "{{qlikSenseQMC}}"
                },
                {
                    "type": "button",
                    "text": {
                        "type": "plain_text",
                        "emoji": true,
                        "text": "Open Hub"
                    },
                    "style": "primary",
                    "url": "{{qlikSenseHub}}"
                },
                {
                    "type": "button",
                    "text": {
                        "type": "plain_text",
                        "emoji": true,
                        "text": "Open app"
                    },
                    "style": "primary",
                    "url": "{{appUrl}}"
                }
{{#each genericUrls}}
{{!-- Possible to use conditional logic in the template to include or exclude buttons based on the value of a variable. For example, the following code snippet includes a button only if the value of the id variable is "butler_docs": --}}
{{!-- {{#if (eq id "butler_docs")}} --}}
                ,{
                    "type": "button",
                    "text": {
                        "type": "plain_text",
                        "emoji": true,
                        "text": "{{this.linkText}}"
                    },
                    "style": "primary",
                    "url": "{{this.url}}"
                }
{{!-- {{/if}} --}}
{{/each}}
            ]
        },
        {
            "type": "divider"
        },
        {
            "type": "section",
            "text": {
                "type": "mrkdwn",
                "text": "```{{ executionDetailsConcatenated }}```"
            }
        },
        {
            "type": "section",
            "text": {
                "type": "mrkdwn",
                "text": "The script log contains {{scriptLogSize}} characters in total. Here are the first lines:"
            }
        },
                {
            "type": "section",
            "text": {
                "type": "mrkdwn",
                "text": "```{{{ scriptLogHead }}}```"
            }
        },
        {
            "type": "section",
            "text": {
                "type": "mrkdwn",
                "text": "Here are the last few lines:"
            }
        },
        {
            "type": "section",
            "text": {
                "type": "mrkdwn",
                "text": "```{{{ scriptLogTail }}}```"
            }
        }
    ]
}
